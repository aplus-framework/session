{
    "name": "aplus/session",
    "description": "Aplus Framework Session Library",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "session",
        "files",
        "database",
        "memcached",
        "redis",
        "cookie"
    ],
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "homepage": "https://aplus-framework.com/packages/session",
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/libraries/session/issues",
        "forum": "https://aplus-framework.com/forum",
        "source": "https://gitlab.com/aplus-framework/libraries/session",
        "docs": "https://docs.aplus-framework.com/guides/libraries/session/"
    },
    "funding": [
        {
            "type": "Aplus Sponsor",
            "url": "https://aplus-framework.com/sponsor"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-memcached": "*",
        "ext-redis": "*",
        "aplus/database": "^3.0",
        "aplus/debug": "^3.1",
        "aplus/log": "^3.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.14",
        "ergebnis/composer-normalize": "^2.25",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Framework\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Session\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
